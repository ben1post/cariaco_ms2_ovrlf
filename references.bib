	abstract = {A new marine ecosystem model designed for the global domain is presented, and model output is compared with ﬁeld data from nine diﬀerent locations. Field data were collected as part of the international Joint Global Ocean Flux Study (JGOFS) program, and from historical time series stations. The ﬁeld data include a wide variety ofmarine ecosystem types, including nitrogen- and iron-limited systems, and diﬀerent physical environments from high latitudes to the mid-ocean gyres. Model output is generally in good agreement with ﬁeld data from these diverse ecosystems. These results imply that the ecosystem model presented here can be reliably applied over the global domain.},
	language = {en},
	number = {1-3},
	urldate = {2025-02-24},
	journal = {Deep Sea Research Part II: Topical Studies in Oceanography},
	author = {Moore, J.Keith and Doney, Scott C. and Kleypas, Joanie A. and Glover, David M. and Fung, Inez Y.},
	month = jan,
	year = {2001},
	pages = {403--462},
}

@article{malerba_larger_2018,
	title = {Do larger individuals cope with resource fluctuations better? {An} artificial selection approach},
	volume = {285},
	issn = {0962-8452, 1471-2954},
	shorttitle = {Do larger individuals cope with resource fluctuations better?},
	url = {https://royalsocietypublishing.org/doi/10.1098/rspb.2018.1347},
	doi = {10.1098/rspb.2018.1347},
	abstract = {Size determines the rate at which organisms acquire and use resources but it is unclear what size should be favoured under unpredictable resource regimes. Some theories claim smaller organisms can grow faster following a resource pulse, whereas others argue larger species can accumulate more resources and maintain growth for longer periods between resource pulses. Testing these theories has relied on interspecific comparisons, which tend to confound body size with other life-history traits. As a more direct approach, we used 280 generations of artificial selection to evolve a 10-fold difference in mean body size between small- and large-selected phytoplankton lineages of
              Dunaliella tertiolecta
              , while controlling for biotic and abiotic variables. We then quantified how body size affected the ability of this species to grow at nutrient-replete conditions and following periods of nitrogen or phosphorous deprivation. Overall, smaller cells showed slower growth, lower storage capacity and poorer recovery from phosphorous depletion, as predicted by the ‘fasting endurance hypothesis'. However, recovery from nitrogen limitation was independent of size—a finding unanticipated by current theories. Phytoplankton species are responsible for much of the global carbon fixation and projected trends of cell size decline could reduce primary productivity by lowering the ability of a cell to store resources.},
	language = {en},
	number = {1884},
	urldate = {2025-02-24},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	author = {Malerba, Martino E. and Palacios, Maria M. and Marshall, Dustin J.},
	month = aug,
	year = {2018},
	pages = {20181347},
}

@article{bruggeman_phylogenetic_2011,
	title = {A {PHYLOGENETIC} {APPROACH} {TO} {THE} {ESTIMATION} {OF} {PHYTOPLANKTON} {TRAITS}$^{\textrm{1}}$},
	volume = {47},
	copyright = {http://onlinelibrary.wiley.com/termsAndConditions\#vor},
	issn = {0022-3646, 1529-8817},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/j.1529-8817.2010.00946.x},
	doi = {10.1111/j.1529-8817.2010.00946.x},
	abstract = {The quantitative characterization of the ecology of individual phytoplankton taxa is essential for model resolution of many aspects of aquatic ecosystems. Existing literature cannot directly parameterize all phytoplankton taxa of interest, as many traits and taxa have not been sampled. However, valuable clues on the value of traits are found in the evolutionary history of species and in common correlations between traits. These two resources were exploited with an existing, statistically consistent method built upon evolutionary concepts. From a new data set with {\textgreater}700 observations on freshwater phytoplankton traits and a qualitative phytoplankton phylogeny, estimates were derived for the size, growth rate, phosphate affinity, and susceptibility to predation of 277 phytoplankton types, from evolutionary ancestors to present‐day species. These estimates account simultaneously for phylogenetic relationships between types, as imposed by the phylogeny, and approximate power‐law relationships (e.g., allometric scaling laws) between traits, as reconstructed from the data set. Results suggest that most phytoplankton traits are to some extent conserved in evolution: cross‐validation demonstrated that the use of phylogenetic information significantly improves trait value estimates. By providing trait value estimates as well as uncertainties, these results could benefit most quantitative studies involving phytoplankton.},
	language = {en},
	number = {1},
	urldate = {2025-02-24},
	journal = {Journal of Phycology},
	author = {Bruggeman, Jorn},
	month = feb,
	year = {2011},
	pages = {52--65},
}

