	author = {Taylor, Gordon T. and Muller-Karger, Frank E. and Thunell, Robert C. and Scranton, Mary I. and Astor, Yrene and Varela, Ramon and Ghinaglia, Luis Troccoli and Lorenzoni, Laura and Fanning, Kent A. and Hameed, Sultan and Doherty, Owen},
