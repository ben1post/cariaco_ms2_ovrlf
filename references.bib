@article{wakeham_biomarkers_2012,
	title = {Biomarkers, chemistry and microbiology show chemoautotrophy in a multilayer chemocline in the {Cariaco} {Basin}},
	volume = {63},
	copyright = {https://www.elsevier.com/tdm/userlicense/1.0/},
